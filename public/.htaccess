Options -MultiViews
RewriteEngine On

##RewriteBase /public/

RewriteCond %{REQUEST_FILENAME} !-d
RewriteCond %{REQUEST_FILENAME} !-f

#RewriteCond %{REQUEST_URI} !^/sitemap.xml$
#RewriteCond %{REQUEST_URI} !^/robots.txt$
RewriteCond %{REQUEST_URI} !^/includes/[^/]+\.*$

RewriteRule ^(.+)$ index.php?url=$1 [QSA,L]